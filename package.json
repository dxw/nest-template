{
  "name": "regulated-professions-register",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:assets": "encore dev",
    "build:assets:prod": "encore production",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -n",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "format:check": "prettier --check \"src/**/*.ts\" \"cypress/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"cypress/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:test": "NODE_ENV=test nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,cypress}/**/*.{ts,js}\" --fix",
    "cy:run": "cypress run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "start-server-and-test start:test http://localhost:3000 cy:run"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/typeorm": "^10.0.0",
    "@symfony/webpack-encore": "^4.5.0",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "file-loader": "^6.2.0",
    "govuk-frontend": "^3.14.0",
    "nestjs-i18n": "^10.3.7",
    "nunjucks": "^3.2.3",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "sass": "^1.43.4",
    "sass-loader": "^13.3.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.6",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@golevelup/ts-jest": "^0.6.0",
    "@nestjs/cli": "^10.2.0",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cypress": "^13.3.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.20",
    "start-server-and-test": "^1.14.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
